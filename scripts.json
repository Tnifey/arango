{
  "$schema": "https://deno.land/x/denon@2.5.0/schema.json",
  "scripts": {
    "dev": {
      "cmd": "deno run test.ts",
      "desc": "run development watch",
      "watch": true,
      "log": "info",
      "tsconfig": "deno.jsonc"
    },
    "test": {
      "cmd": "deno test",
      "desc": "run tests",
      "watch": false
    }
  },
  "allow": [
    "net",
    "env",
    "read"
  ]
}
